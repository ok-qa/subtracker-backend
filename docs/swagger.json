    "/api/subscriptions": {
      "get": {
        "tags": [
          "Subscriptions"
        ],
        "summary": "Get all subscriptions",
        "operationId": "getSubscriptions",
        "description": "Retrieve a list of subscriptions for the authenticated user",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "in": "query",
            "name": "page",
            "schema": {
              "type": "integer"
            },
            "example": 1
          },
          {
            "in": "query",
            "name": "perPage",
            "schema": {
              "type": "integer",
              "example": 5
            }
          },
          {
            "in": "query",
            "name": "sortBy",
            "description": "Sort by fields: \n _id (default value),\n name,\n price,\n endDate,\n createdAt,\n updatedAt.\n",
            "schema": {
              "type": "string",
              "example": "price"
            }
          },
          {
            "in": "query",
            "name": "sortOrder",
            "description": "Sort order values:\n  asc,\n  desc.\n",
            "schema": {
              "type": "string",
              "example": "asc"
            }
          },
          {
            "in": "query",
            "name": "name",
            "description": "filter subscriptions by name, case insensitive",
            "schema": {
              "type": "string",
              "example": "Netflix"
            }
          },
          {
            "in": "query",
            "name": "term",
            "description": "Filter subscriptions by term.\nTerm values: year, month, trial\n",
            "schema": {
              "type": "string",
              "example": "year"
            }
          },
          {
            "in": "query",
            "name": "categoryIds",
            "description": "Filter subscriptions by category.\nCategory values:  \n  1 - Entertainment | 2 - Financial & Utilities | 3 - Food & Drinks | 4 - Gaming | 5 - Cloud Storage |  \n  6 - Mobile and Internet | 7 - Software & Productivity | 8 - Shopping & Memberships | 9 - Pets |  \n  10 - Health & Fitness | 11 - News | 12 - Education | 13 - Food,\n",
            "schema": {
              "oneOf": [
                {
                  "type": "string"
                },
                {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              ],
              "example": [
                "6",
                "12"
              ]
            }
          },
          {
            "in": "query",
            "name": "minPrice",
            "description": "filter subscriptions by minPrice >= minPrice",
            "schema": {
              "type": "string",
              "example": "3.99"
            }
          },
          {
            "in": "query",
            "name": "maxPrice",
            "description": "filter subscriptions by maxPrice <= maxPrice",
            "schema": {
              "type": "string",
              "example": "100"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of subscriptions",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/subscription"
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/401"
          }
        }
      },
      "post": {
        "tags": [
          "Subscriptions"
        ],
        "summary": "Create subscription",
        "operationId": "createSubscription",
        "description": "Create a new subscription",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/createSubscriptionRequest"
              }
            }
          }
        },
        "responses": {
